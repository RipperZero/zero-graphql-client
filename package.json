    "@mui/styles": "^5.10.10",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.6",
    "graphql": "^16.6.0",
    "immer": "^9.0.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
